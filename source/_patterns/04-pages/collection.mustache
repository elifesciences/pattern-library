<div class="global-wrapper">

  <div class="global-inner">

    {{#siteHeader}}
      {{> organisms-site-header }}
    {{/siteHeader}}

    <main role="main">

    {{#contentHeader}}
      {{> organisms-pl-only-content-header-collection}}
    {{/contentHeader}}


    <div class="wrapper">

      {{#leadParas}}
        {{> molecules-lead-paras}}
      {{/leadParas}}

      <div class="grid">

        <div class="listing-column-container">

          <div class="grid__item large--eight-twelfths listing-column-main">

            {{#mainTeasers}}
              {{> organisms-list}}
            {{/mainTeasers}}

          </div>

          <div class="grid__item large--four-twelfths listing-column-secondary">

            {{#multimediaTeasers}}
              {{> organisms-list}}
            {{/multimediaTeasers}}

            {{#relatedTeasers}}
              {{> organisms-list}}
            {{/relatedTeasers}}

            {{#profileLinks}}
              {{> organisms-list}}
            {{/profileLinks}}

          </div>

        </div>

      </div>

    </div>

  </main>

  {{> organisms-email-cta }}

  {{> organisms-footer }}

  </div>

</div>
