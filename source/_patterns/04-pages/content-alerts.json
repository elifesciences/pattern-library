    "body": "<form id=\"content_alerts\" action=\"/content-alerts\" method=\"POST\" novalidate=\"\">    <div class=\"form-item\">  <div class=\"form-item__label_container\">      <label for=\"content_alerts_email\" class=\"form-item__label\">        Email      </label>      <div class=\"form-field-info-link-wrapper\">        <a class=\"form-field-info-link\" href=\"/privacy\">Privacy notice</a>      </div>        </div>  <input type=\"email\" class=\"text-field text-field--email\" id=\"content_alerts_email\" name=\"content_alerts[email]\" required=\"required\"></div><div class=\"form-item\">    <div class=\"form-item__title\">I would like to receive the following regular emails from eLife:</div>  <div class=\"checkboxes\" id=\"content_alerts_preferences\">    <ul class=\"checkboxes__list\">        <li class=\"checkboxes__list_item\">            <label class=\"checkboxes__item_label\">              <input type=\"checkbox\" name=\"content_alerts[preferences][]\" value=\"latest_articles\" id=\"content_alerts_preferences\" checked=\"\" required=\"required\">              <span class=\"checkbox__item\">The latest scientific articles published by eLife (twice weekly)</span>            </label>        </li>        <li class=\"checkboxes__list_item\">            <div class=\"form-item__sub_title\">Our other newsletters</div>            <ul class=\"checkboxes__children_list\">                <li class=\"checkboxes__children_list_item\">                  <label class=\"checkboxes__item_label\">                    <input type=\"checkbox\" name=\"content_alerts[preferences][]\" value=\"early_career\" id=\"content_alerts_preferences\" required=\"required\">                    <span class=\"checkbox__item\">Early-career researchers newsletter (monthly)</span>                  </label>                </li>                <li class=\"checkboxes__children_list_item\">                  <label class=\"checkboxes__item_label\">                    <input type=\"checkbox\" name=\"content_alerts[preferences][]\" value=\"technology\" id=\"content_alerts_preferences\" required=\"required\">                    <span class=\"checkbox__item\">Technology and Innovation newsletter (every two months)</span>                  </label>                </li>                <li class=\"checkboxes__children_list_item\">                  <label class=\"checkboxes__item_label\">                    <input type=\"checkbox\" name=\"content_alerts[preferences][]\" value=\"elife_newsletter\" id=\"content_alerts_preferences\" required=\"required\">                    <span class=\"checkbox__item\">eLife newsletter (every two months)</span>                  </label>                </li>            </ul>        </li>    </ul>  </div></div>  <button class=\"button button--default\" type=\"submit\" id=\"content_alerts_subscribe\" name=\"content_alerts[subscribe]\">Subscribe</button><div class=\"form-item visuallyhidden\" aria-hidden=\"true\">    <label for=\"content_alerts_email_address\" class=\"form-item__label\">Please leave this field empty</label>  <input type=\"email\" class=\"text-field text-field--email\" id=\"content_alerts_email_address\" name=\"content_alerts[email_address]\" tabindex=\"-1\"></div></form>",
  
