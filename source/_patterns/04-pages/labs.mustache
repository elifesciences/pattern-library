<div class="global-wrapper">

  <div class="global-inner">

    <div class="wrapper wrapper--site-header">
      {{#siteHeader}}
        {{> organisms-site-header }}
      {{/siteHeader}}
    </div>

    <main role="main" class="main">

      {{#contentHeader}}
        {{> organisms-content-header}}
      {{/contentHeader}}

      <div class="wrapper wrapper--listing">

        <div class="grid">

          <div class="grid__item one-whole grid-column">

          {{#listHeading}}
            {{> atoms-list-heading}}
          {{/listHeading}}

          {{#labsListing}}
            {{> organisms-grid-listing}}
          {{/labsListing}}

          </div>

        </div>

      </div>

    </main>

    {{#emailCta}}
      {{> organisms-email-cta }}
    {{/emailCta}}

    {{> organisms-footer }}

  </div>

</div>
