<div class="global-wrapper">
  <div class="global-inner">
    {{#siteHeader}}
      {{> organisms-site-header }}
    {{/siteHeader}}

    <main role="main" class="blg-margin-bottom-large">

      {{#carousel}}
        {{> organisms-carousel}}
      {{/carousel}}

      {{#leadParas}}
        {{> molecules-lead-paras}}
      {{/leadParas}}

      {{#subjectListLink}}
      <div class="wrapper u-pad-top">
        {{> atoms-all-subjects-list-link}}
      </div>
      {{/subjectListLink}}

      <div class="wrapper u-pad-top">
        <div style="height: 800px; background-color: rgba(2, 136, 209, 0.2);">Main article list.</div>
      </div>

      {{#subjectsList}}
        {{> molecules-all-subjects-list}}
      {{/subjectsList}}

    </main>

    {{#emailCta}}
    {{> organisms-email-cta }}
    {{/emailCta}}

    {{> organisms-footer }}
  </div> <!-- /.global-inner -->
</div> <!-- /.global-wrapper -->
