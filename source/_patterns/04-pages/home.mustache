<div class="global-wrapper">
  <div class="global-inner">

    <div class="wrapper wrapper--site-header">
      {{#siteHeader}}
        {{> organisms-site-header }}
      {{/siteHeader}}
    </div>

    <main role="main" class="main blg-margin-bottom-large">

      {{#carousel}}
        {{> organisms-carousel}}
      {{/carousel}}

      {{#leadParas}}
        <div class="wrapper">
          {{> molecules-lead-paras}}
        </div>
      {{/leadParas}}

      {{#sectionsListingLink}}
      <div class="wrapper">
        {{> atoms-section-listing-link}}
      </div>
      {{/sectionsListingLink}}

      <div class="wrapper blg-pad-top-medium">
        <div class="grid">
          <div class="listing-column-container">
            <div class="grid__item one-whole x-large--eight-twelfths listing-column-main">

              {{#mainTeasers}}
                {{> organisms-listing-teasers}}
              {{/mainTeasers}}

            </div>
            <div class="grid__item one-whole x-large--four-twelfths listing-column-secondary">

              {{#secondaryTeasers}}
                {{> organisms-listing-teasers}}
              {{/secondaryTeasers}}

            </div>
          </div>
        </div>
      </div>


      {{#sectionsListing}}
        <div class="wrapper u-pad-top-lg">
          {{> molecules-section-listing}}
        </div>
      {{/sectionsListing}}

    </main>

    {{#emailCta}}
    {{> organisms-email-cta }}
    {{/emailCta}}

    {{> organisms-footer }}
  </div> {{! /.global-inner }}
</div> {{! /.global-wrapper }}
