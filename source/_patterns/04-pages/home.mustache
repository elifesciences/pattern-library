<div class="global-wrapper">
  <div class="global-inner">

    <div class="wrapper wrapper--site-header">
      {{#siteHeader}}
        {{> organisms-site-header }}
      {{/siteHeader}}
    </div>

    <main role="main" class="main" id="maincontent">

      {{#hero-banner}}
        <div class="wrapper">
            {{> organisms-hero-banner}}
        </div>
      {{/hero-banner}}

      {{#leadParas}}
        <div class="wrapper">
          {{> molecules-lead-paras}}
        </div>
      {{/leadParas}}

      {{#sectionsListingLink}}
      <div class="wrapper">
        {{> atoms-section-listing-link}}
      </div>
      {{/sectionsListingLink}}

      <div class="wrapper wrapper--listing">
        <div class="grid">
          <div class="grid__item one-whole x-large--eight-twelfths listing-column-main">

            {{#mainTeasers}}
              {{> organisms-listing-teasers}}
            {{/mainTeasers}}

          </div>

          <div class="grid__item one-whole x-large--four-twelfths listing-column-secondary">

              {{#secondaryTeasers}}
                {{> organisms-listing-teasers}}
              {{/secondaryTeasers}}

          </div>

          <div>
      </div>


      {{#sectionsListing}}
        <div class="wrapper u-pad-top-lg">
          {{> molecules-section-listing}}
        </div>
      {{/sectionsListing}}

    </main>

    {{#emailCta}}
    {{> organisms-email-cta }}
    {{/emailCta}}

    {{> organisms-footer }}
  </div> {{! /.global-inner }}
</div> {{! /.global-wrapper }}
