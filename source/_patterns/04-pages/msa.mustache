<div class="global-wrapper">

  <div class="global-inner">

    <div class="wrapper wrapper--site-header">
      {{#siteHeader}}
        {{> organisms-site-header }}
      {{/siteHeader}}
    </div>

    <main role="main" class="main" id="maincontent">

    {{#contentHeader}}
      {{> organisms-content-header}}
    {{/contentHeader}}

      <div class="wrapper">
        <div class="listing-column-container listing-column-main">
          {{#highlightsTeasers}}
            {{> organisms-listing-teasers}}
          {{/highlightsTeasers}}

        </div>
      </div>


    <div class="wrapper wrapper--listing wrapper--listing-selector">

      <div class="grid">

        <div class="listing-column-container">

          {{#viewSelector}}
            {{> molecules-view-selector }}
          {{/viewSelector}}

          <div class="grid__item large--eight-twelfths listing-column-main primary-column" id="primaryListing">

            {{#mainTeasers}}
              {{> organisms-listing-teasers}}
            {{/mainTeasers}}

          </div>

          <div class="grid__item large--four-twelfths listing-column-secondary secondary-column" id="secondaryListing">

            {{#secondaryTeasers}}
              {{> organisms-listing-teasers}}
            {{/secondaryTeasers}}

            {{#profileLinks}}
              {{> organisms-listing-profile-snippets}}
            {{/profileLinks}}

          </div>

        </div>

      </div>

    </div>

  </main>

    {{#emailCta}}
      {{> organisms-email-cta }}
    {{/emailCta}}

    {{> organisms-footer }}

  </div>

</div>
