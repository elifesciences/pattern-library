{{> atoms-list-heading}}
<ol class="listing-list">
  {{#items}}
      <li class="listing-list__item">
        {{> molecules-teaser }}
    </li>
  {{/items}}

  {{#seeMoreLink}}
    <li class="listing-list__item">
      {{> atoms-see-more-link}}
    </li>
  {{/seeMoreLink}}

</ol>

{{#pagination}}
  {{^previousPage}}
    {{> molecules-load-more}}
  {{/previousPage}}
  {{#previousPage}}
    {{> molecules-pager}}
  {{/previousPage}}
{{/pagination}}
