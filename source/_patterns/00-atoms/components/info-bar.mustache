<div class="info-bar info-bar--{{type}}" id="{{id}}"
     {{#dismissible}}
        data-behaviour="InfoBar"
        {{#cookieDuration}} data-cookie-name-root="infoBar_" data-cookie-duration="{{cookieDuration}}"{{/cookieDuration}}
        {{#cookieExpires}} data-behaviour="InfoBar" data-cookie-name-root="infoBar_" data-cookie-expires="{{cookieExpires}}"{{/cookieExpires}}>
     {{/dismissible}}

    <div class="info-bar__container">

      <div class="info-bar__text">{{{text}}}</div>

    </div>

</div>
