<div class="info-bar info-bar--{{type}}" id="{{id}}" data-behaviour="InfoBar" data-cookie-name-root="infoBar_"
  {{#cookieDuration}} data-cookie-duration="{{cookieDuration}}"{{/cookieDuration}}
  {{#cookieExpires}} data-cookie-expires="{{cookieExpires}}"{{/cookieExpires}}>

  {{#dismissibleTrue}}
    <div class="info-bar__container">
  {{/dismissibleTrue}}
      <div class="info-bar__text">{{{text}}}</div>
  {{#dismissibleTrue}}
    </div>
  {{/dismissibleTrue}}
</div>
