<div
  id="highcharts-container"
  data-behaviour="Metrics"
  data-id="09560"
  data-type="article"
  data-per-view="30"
  data-container-id="chart-1"
  data-daily-id="{{daily.id}}"
  data-monthly-id="{{monthly.id}}"
  data-api-endpoint="http://prod--gateway.elifesciences.org"
>
  <div class="charts">
    <div id="chart-1">Loading...</div>
    <p>
      <span class="visuallyhidden">Toggle charts</span>
      <a class="trigger trigger--prev" href="#">
        {{#prev}}{{>atoms-picture}}{{/prev}}
      </a>
      <a class="trigger trigger--next" href="#">
        {{#next}}{{>atoms-picture}}{{/next}}
      </a>
    </p>
  </div>
  <div class="button-collection clearfix">
    {{#daily.text}}
      {{#daily}}{{>atoms-button}}{{/daily}}
    {{/daily.text}}

    {{#monthly.text}}
      {{#monthly}}{{>atoms-button}}{{/monthly}}
    {{/monthly.text}}
  </div>
</div>
