<div class="view-selector{{#figureUrl}} view-selector--has-figures{{/figureUrl}}" data-behaviour="ViewSelector"{{#sideBySideUrl}} data-side-by-side-link="{{sideBySideUrl}}"{{/sideBySideUrl}}>
  <ul class="view-selector__list">
    <li class="view-selector__list-item view-selector__list-item--article{{^figureIsActive}} view-selector__list-item--active{{/figureIsActive}}">
      <a href="{{articleUrl}}" class="view-selector__link view-selector__link--article"><span>Article</span></a>
    </li>
    {{#figureUrl}}
      <li class="view-selector__list-item view-selector__list-item--figures{{#figureIsActive}} view-selector__list-item--active{{/figureIsActive}}">
        <a href="{{figureUrl}}" class="view-selector__link view-selector__link--figures"><span>Figures and data</span></a>
      </li>
    {{/figureUrl}}

    {{#otherLinks}}
      <li class="view-selector__list-item">
        <a href="{{url}}" class="view-selector__link"><span>{{{name}}}</span></a>
      </li>
    {{/otherLinks}}

    <li class="view-selector__list-item view-selector__divider"></li>

    {{#jumpLinks}}
      {{#links}}
        <li class="view-selector__jump_link_item view-selector__list-item">
          <a href="{{url}}" class="view-selector__jump_link">{{{name}}}</a>
        </li>
      {{/links}}
    {{/jumpLinks}}

  </ul>
</div>
