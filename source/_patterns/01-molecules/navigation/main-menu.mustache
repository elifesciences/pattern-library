
<div class="main-menu" id="mainMenu" data-behaviour="MainMenu">
  <nav class="main-menu__container" role="navigation">
    {{#mainMenuLinks}}
      <ul class="main-menu__list">
        {{#items}}
        <li class="main-menu__list_item">
          <a href="{{url}}" class="main-menu__list_link">{{name}}</a>
        </li>
        {{/items}}
      </ul>
    {{/mainMenuLinks}}
  </nav>
  <a href="#siteHeader" class="main_menu__quit">Back to top</a>
</div>
