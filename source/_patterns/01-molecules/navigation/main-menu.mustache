
<div class="main-menu">
  <div class="main-menu__container">
    {{#mainmenulinks}}
    <div class="main-menu__section">
      <h4 class="main-menu__title">{{title}}</h4>
      <ul class="main-menu__list">
        {{#items}}
        <li class="main-menu__list_item">
          <a href="{{url}}" class="main-menu__list_link">{{name}}</a>
        </li>
        {{/items}}
      </ul>
    </div>
    {{/mainmenulinks}}
  </div>
</div>
